{
  "manifest_version": 3,
  "name": "CyberSentinel Extension",
  "version": "1.0",
  "description": "Detects and sends suspicious URLs to backend",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}