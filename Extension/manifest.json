{
  "manifest_version": 3,
  "name": "CyberSentinel Extension",
  "version": "1.0",
  "description": "Blocks malicious domains using DNR and background scripts.",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "downloads",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
